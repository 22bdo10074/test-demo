// here is my file1 content 
#include <stdio.h>

int main() {
    int num-1, num-2, sum;

    // Ask the user to enter the first number
    printf("Enter the first number: ");
    scanf("%d", &num-1);

    // Ask the user to enter the second number
    printf("Enter the second number: ");
    scanf("%d", &num-2);

    // Calculate the sum
    sum = num-1 + num-2;

    // Print the sum
    printf("The sum of %d and %d is %d\n", num-1, num-2, sum);

    return 0;
}
